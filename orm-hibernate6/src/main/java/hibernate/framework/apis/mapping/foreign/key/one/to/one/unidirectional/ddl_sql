Hibernate:
    create table public.t_fk_idcard (
       id integer not null,
        code varchar(255),
        primary key (id)
    )
Hibernate:
    create table public.t_fk_person (
       id integer not null,
        name varchar(255),
        age integer,
        primary key (id)
    )

Hibernate:
    insert
    into
        public.t_fk_person
        (age, name, id)
    values
        (?, ?, ?)
