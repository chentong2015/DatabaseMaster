<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 在类名称层级上需要添加entity-name=""避免JPA entity name名称冲突 -->
<hibernate-mapping package="hibernate.framework.apis.associations.unidirectional.many.to.one">
    <class name="Grade" entity-name="hibernate.framework.apis.associations.unidirectional.many.to.one.Grade" schema="public"
           table="t_grade">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name" column="name"/>
    </class>
</hibernate-mapping>