<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.framework.apis.mapping.many.to.many">
    <class name="Typology" schema="public" table="t_typology">
        <id name="id" column="id"/>
        <property name="name" column="name"/>

        <set name="strategies" table="t_typology_set_strategies">
            <key column="m_key_typology"/>
            <many-to-many outer-join="auto" class="Strategy" column="m_key_strategy"/>
        </set>
    </class>
</hibernate-mapping>