// 首先创建主表和"集合组件"关联表
// 设置关联表的外键references到主表的主键
Hibernate:
    create table t_developer (
       id int8 not null,
        name varchar(255),
        primary key (id)
    )
Hibernate:
    create table t_developer_some_names (
       id int8 not null,
        initial char(1),
        first varchar(255),
        last varchar(255)
    )
Hibernate:
    alter table t_developer_some_names
       add constraint FKbmvvtf00wgy1mn4g1vj707ay9
       foreign key (id)
       references t_developer


// 在插入主表数据的同时，同时插入"集合组件"关联表数据
Hibernate:
    insert
    into
        t_developer
        (name, id)
    values
        (?, ?)
Hibernate:
    insert
    into
        t_developer_some_names
        (id, initial, first, last)
    values
        (?, ?, ?, ?)
Hibernate:
    insert
    into
        t_developer_some_names
        (id, initial, first, last)
    values
        (?, ?, ?, ?)
Hibernate:
    insert
    into
        t_developer_some_names
        (id, initial, first, last)
    values
        (?, ?, ?, ?)