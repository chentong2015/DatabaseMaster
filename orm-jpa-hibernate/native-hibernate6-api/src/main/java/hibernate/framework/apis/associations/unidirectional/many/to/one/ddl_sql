// 1. 先创建数据表
Hibernate:
    create table public.t_grade (
       id integer not null,
        name varchar(255),
        primary key (id)
    )
Hibernate:
    create table public.t_student (
       id integer not null,
        name varchar(255),
        age integer,
        grade_id integer not null,
        primary key (id)
    )

// 2. 在插入One-端的数据
Hibernate:
    insert
    into
        public.t_grade
        (name, id)
    values
        (?, ?)
Hibernate:
    insert
    into
        public.t_grade
        (name, id)
    values
        (?, ?)

// 3. 最后插入Many-端的数据: 直接设置外键的值，无需update操作
Hibernate:
    insert
    into
        public.t_student
        (age, grade_id, name, id)
    values
        (?, ?, ?, ?)
Hibernate:
    insert
    into
        public.t_student
        (age, grade_id, name, id)
    values
        (?, ?, ?, ?)
Hibernate:
    insert
    into
        public.t_student
        (age, grade_id, name, id)
    values
        (?, ?, ?, ?)
