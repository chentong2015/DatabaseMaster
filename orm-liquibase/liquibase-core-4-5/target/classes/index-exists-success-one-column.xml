<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <changeSet author="DAS" id="IndexDoesExistOneColumn">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="MY_TABLE_NAME"/>
            <not>
                <indexExists tableName="MY_TABLE_NAME" columnNames="M_LABEL"/>
            </not>
        </preConditions>
        <createTable tableName="TABLE_SHOULD_NOT_BE_CREATED_ONE_COLUMN">
            <column name="M_ID" type="int" />
        </createTable>
    </changeSet>

</databaseChangeLog>