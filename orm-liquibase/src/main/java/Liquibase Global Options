      --strict=PARAM         Be stricter on allowed Liquibase configuration and setup?
                             DEFAULT: false
                             (liquibase.strict)
                             (LIQUIBASE_STRICT)

      --support-property-escaping=PARAM

                             Support escaping changelog parameters using a colon. Example: null
                             DEFAULT: false
                             (liquibase.supportPropertyEscaping)
                             (LIQUIBASE_SUPPORT_PROPERTY_ESCAPING)
                             [deprecated: --supportPropertyEscaping]

      --use-procedure-schema=PARAM

                             If set to true (default value), createProcedure tags with a set schemaName will modify the procedure
                               body with the given schema name.
                             DEFAULT: true
                             (liquibase.useProcedureSchema)
                             (LIQUIBASE_USE_PROCEDURE_SCHEMA)
                             [deprecated: --useProcedureSchema]

  -v, --version              Print version information and exit