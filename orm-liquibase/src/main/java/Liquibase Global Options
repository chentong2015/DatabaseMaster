Global Options 所有的全局参数选项

      --always-override-stored-logic-schema=PARAM
                             When generating SQL for createProcedure, should the procedure schema be forced to the default schema if
                               no schemaName attribute is set?
                             DEFAULT: false
                             (liquibase.alwaysOverrideStoredLogicSchema)
                             (LIQUIBASE_ALWAYS_OVERRIDE_STORED_LOGIC_SCHEMA)
                             [deprecated: --alwaysOverrideStoredLogicSchema]

      --auto-reorg=PARAM
                             Should Liquibase automatically include REORG TABLE commands when needed?
                             DEFAULT: true
                             (liquibase.autoReorg)
                             (LIQUIBASE_AUTO_REORG)
                             [deprecated: --autoReorg]

      --changelog-lock-poll-rate=PARAM

                             Number of seconds wait between checks to the changelog lock when it is locked
                             DEFAULT: 10
                             (liquibase.changelogLockPollRate)
                             (LIQUIBASE_CHANGELOG_LOCK_POLL_RATE)
                             [deprecated: --changelogLockPollRate]

      --changelog-lock-wait-time-in-minutes=PARAM

                             Number of minutes to wait for the changelog lock to be available before giving up
                             DEFAULT: 5
                             (liquibase.changelogLockWaitTimeInMinutes)
                             (LIQUIBASE_CHANGELOG_LOCK_WAIT_TIME_IN_MINUTES)
                             [deprecated: --changelogLockWaitTimeInMinutes]

      --classpath=PARAM      Additional classpath entries to use
                             (liquibase.classpath)
                             (LIQUIBASE_CLASSPATH)

      --convert-data-types=PARAM

                             Should Liquibase convert to/from STANDARD data types. Applies to both snapshot and update commands.
                             DEFAULT: true
                             (liquibase.convertDataTypes)
                             (LIQUIBASE_CONVERT_DATA_TYPES)
                             [deprecated: --convertDataTypes]

      --database-changelog-lock-table-name=PARAM

                             Name of table to use for tracking concurrent Liquibase usage
                             DEFAULT: DATABASECHANGELOGLOCK
                             (liquibase.databaseChangelogLockTableName)
                             (LIQUIBASE_DATABASE_CHANGELOG_LOCK_TABLE_NAME)
                             [deprecated: --databaseChangelogLockTableName]

      --database-changelog-table-name=PARAM

                             Name of table to use for tracking change history
                             DEFAULT: DATABASECHANGELOG
                             (liquibase.databaseChangelogTableName)
                             (LIQUIBASE_DATABASE_CHANGELOG_TABLE_NAME)
                             [deprecated: --databaseChangelogTableName]

      --database-class=PARAM
                             Class to use for Database implementation
                             (liquibase.databaseClass)
                             (LIQUIBASE_DATABASE_CLASS)
                             [deprecated: --databaseClass]

      --ddl-lock-timeout=PARAM

                             The DDL_LOCK_TIMEOUT parameter indicates the number of seconds a DDL command should wait for the locks
                               to become available before throwing the resource busy error message. This applies only to Oracle
                               databases.
                             (liquibase.ddlLockTimeout)
                             (LIQUIBASE_DDL_LOCK_TIMEOUT)
                             [deprecated: --ddlLockTimeout]

      --defaultlogger-level=PARAM

                             Logging level
                             DEFAULT: INFO
                             (liquibase.defaultlogger.level)
                             (LIQUIBASE_DEFAULTLOGGER_LEVEL)
                             [deprecated: --defaultloggerLevel]

      --defaults-file=PARAM
                             File with default Liquibase properties
                             DEFAULT: liquibase.properties
                             (liquibase.defaultsFile)
                             (LIQUIBASE_DEFAULTS_FILE)
                             [deprecated: --defaultsFile]

      --diff-column-order=PARAM

                             Should Liquibase compare column order in diff operation?
                             DEFAULT: true
                             (liquibase.diffColumnOrder)
                             (LIQUIBASE_DIFF_COLUMN_ORDER)
                             [deprecated: --diffColumnOrder]

      --driver=PARAM         Database driver class
                             (liquibase.driver)
                             (LIQUIBASE_DRIVER)

      --driver-properties-file=PARAM

                             Driver-specific properties
                             (liquibase.driverPropertiesFile)
                             (LIQUIBASE_DRIVER_PROPERTIES_FILE)
                             [deprecated: --driverPropertiesFile]

      --file-encoding=PARAM
                             Encoding to use when reading files. Valid values include: UTF-8, UTF-16, UTF-16BE, UTF-16LE, US-ASCII,
                               or OS to use the system configured encoding.
                             DEFAULT: UTF-8
                             (liquibase.fileEncoding)
                             (LIQUIBASE_FILE_ENCODING)
                             [deprecated: --fileEncoding]

      --filter-log-messages=PARAM

                             DEPRECATED: No longer used
                             (liquibase.filterLogMessages)
                             (LIQUIBASE_FILTER_LOG_MESSAGES)
                             [deprecated: --filterLogMessages]

      --generate-changeset-created-values=PARAM

                             Should Liquibase include a 'created' attribute in diff/generateChangelog changesets with the current
                               datetime
                             DEFAULT: false
                             (liquibase.generateChangesetCreatedValues)
                             (LIQUIBASE_GENERATE_CHANGESET_CREATED_VALUES)
                             [deprecated: --generateChangesetCreatedValues]

      --generated-changeset-ids-contains-description=PARAM

                             Should Liquibase include the change description in the id when generating changesets?
                             DEFAULT: false
                             (liquibase.generatedChangesetIdsContainsDescription)
                             (LIQUIBASE_GENERATED_CHANGESET_IDS_CONTAINS_DESCRIPTION)
                             [deprecated: --generatedChangesetIdsContainsDescription]

  -h, --help                 Show this help message and exit
      --headless=PARAM       Force liquibase to think it has no access to a keyboard
                             DEFAULT: false
                             (liquibase.headless)
                             (LIQUIBASE_HEADLESS)

      --hub-api-key=PARAM
                             Liquibase Hub API key for operations
                             (liquibase.hub.apiKey)
                             (LIQUIBASE_HUB_API_KEY)
                             [deprecated: --hubApiKey]

      --hub-log-level=PARAM
                             Log level for filtering log messages to send to Liquibase Hub during operations. Values can be any
                               acceptable log level.
                             DEFAULT: INFO
                             (liquibase.hub.logLevel)
                             (LIQUIBASE_HUB_LOG_LEVEL)
                             [deprecated: --hubLogLevel]

      --hub-mode=PARAM
                             Content to send to Liquibase Hub during operations. Values can be 'all', 'meta', or 'off'
                             DEFAULT: ALL
                             (liquibase.hub.mode)
                             (LIQUIBASE_HUB_MODE)
                             [deprecated: --hubMode]

      --hub-url=PARAM
                             Liquibase Hub URL for operations
                             DEFAULT: https://hub.liquibase.com
                             (liquibase.hub.url)
                             (LIQUIBASE_HUB_URL)
                             [deprecated: --hubUrl]

      --include-catalog-in-specification=PARAM

                             Should Liquibase include the catalog name when determining equality?
                             DEFAULT: false
                             (liquibase.includeCatalogInSpecification)
                             (LIQUIBASE_INCLUDE_CATALOG_IN_SPECIFICATION)
                             [deprecated: --includeCatalogInSpecification]

      --include-system-classpath=PARAM

                             Include the system classpath when resolving classes at runtime
                             DEFAULT: true
                             (liquibase.includeSystemClasspath)
                             (LIQUIBASE_INCLUDE_SYSTEM_CLASSPATH)
                             [deprecated: --includeSystemClasspath]

      --liquibase-catalog-name=PARAM

                             Catalog to use for Liquibase objects
                             (liquibase.liquibaseCatalogName)
                             (LIQUIBASE_LIQUIBASE_CATALOG_NAME)
                             [deprecated: --liquibaseCatalogName]

      --liquibase-schema-name=PARAM

                             Schema to use for Liquibase objects
                             (liquibase.liquibaseSchemaName)
                             (LIQUIBASE_LIQUIBASE_SCHEMA_NAME)
                             [deprecated: --liquibaseSchemaName]

      --liquibase-tablespace-name=PARAM

                             Tablespace to use for Liquibase objects
                             (liquibase.liquibaseTablespaceName)
                             (LIQUIBASE_LIQUIBASE_TABLESPACE_NAME)
                             [deprecated: --liquibaseTablespaceName]

      --log-channels=PARAM
                             DEFAULT: Controls which log channels have their level set by the liquibase.logLevel setting. Comma
                               separate multiple values. To set the level of all channels, use 'all'. Example: liquibase,org.mariadb.
                               jdbc
                             (liquibase.logChannels)
                             (LIQUIBASE_LOG_CHANNELS)
                             [deprecated: --logChannels]

      --log-file=PARAM
                             (liquibase.logFile)
                             (LIQUIBASE_LOG_FILE)
                             [deprecated: --logFile]

      --log-level=PARAM
                             DEFAULT: Controls which logs get set to stderr AND to any log file. The CLI defaults, if log file set,
                               to SEVERE. Others vary by integration. The official log levels are: OFF, SEVERE, WARNING, INFO, FINE
                             (liquibase.logLevel)
                             (LIQUIBASE_LOG_LEVEL)
                             [deprecated: --logLevel]

      --monitor-performance=PARAM

                             Enable performance tracking. Set to 'false' to disable. If set to 'true', data is stored to a
                               `liquibase-TIMESTAMP.jfr` file in your working directory. Any other value will enable tracking and be
                               used as the name of the file to write the data to.
                             DEFAULT: false
                             (liquibase.monitorPerformance)
                             (LIQUIBASE_MONITOR_PERFORMANCE)
                             [deprecated: --monitorPerformance]

      --native-executor=PARAM

                             Custom executor that can specified
                             (liquibase.native.executor)
                             (LIQUIBASE_NATIVE_EXECUTOR)
                             [deprecated: --nativeExecutor]

      --output-file=PARAM
                             (liquibase.outputFile)
                             (LIQUIBASE_OUTPUT_FILE)
                             [deprecated: --outputFile]

      --output-file-encoding=PARAM

                             Encoding to use when writing files
                             DEFAULT: UTF-8
                             (liquibase.outputFileEncoding)
                             (LIQUIBASE_OUTPUT_FILE_ENCODING)
                             [deprecated: --outputFileEncoding]

      --output-line-separator=PARAM

                             Line separator for output
                             DEFAULT: Line separator(LF or CRLF) for output. Defaults to OS default
                             (liquibase.outputLineSeparator)
                             (LIQUIBASE_OUTPUT_LINE_SEPARATOR)
                             [deprecated: --outputLineSeparator]

      --pro-license-key=PARAM

                             Liquibase Pro license key used to unlock paid capabilities. Get a free trial at
                              https://www.liquibase.com/protrial and use in CLI or add liquibase.pro.licenseKey=<yourKey> into your
                               defaults file.
                             (liquibase.pro.licenseKey)
                             (LIQUIBASE_PRO_LICENSE_KEY)
                             [deprecated: --proLicenseKey]

      --pro-mark-unused-not-drop=PARAM

                             If a column would be dropped in a diffChangeLog, call markUnused instead if set to true
                             DEFAULT: false
                             (liquibase.pro.markUnusedNotDrop)
                             (LIQUIBASE_PRO_MARK_UNUSED_NOT_DROP)
                             [deprecated: --proMarkUnusedNotDrop]

      --pro-sql-inline=PARAM
                             If true, generate changeSets with SQL-based changes inlined instead of saving them to an external file
                             DEFAULT: false
                             (liquibase.pro.sql.inline)
                             (LIQUIBASE_PRO_SQL_INLINE)
                             [deprecated: --proSqlInline]

      --pro-synonyms-drop-public=PARAM

                             If false, do not drop public synonyms in diffChangeLog/dropAll
                             DEFAULT: true
                             (liquibase.pro.synonyms.drop.public)
                             (LIQUIBASE_PRO_SYNONYMS_DROP_PUBLIC)
                             [deprecated: --proSynonymsDropPublic]

      --prompt-for-non-local-database=PARAM

                             Should Liquibase prompt if a non-local database is being accessed
                             (liquibase.promptForNonLocalDatabase)
                             (LIQUIBASE_PROMPT_FOR_NON_LOCAL_DATABASE)
                             [deprecated: --promptForNonLocalDatabase]

      --property-provider-class=PARAM

                             Implementation of Properties class to provide additional driver properties
                             (liquibase.propertyProviderClass)
                             (LIQUIBASE_PROPERTY_PROVIDER_CLASS)
                             [deprecated: --propertyProviderClass]

      --secure-parsing=PARAM
                             If true, remove functionality from file parsers which could be used insecurely. Examples include (but
                               not limited to) disabling remote XML entity support.
                             DEFAULT: true
                             (liquibase.secureParsing)
                             (LIQUIBASE_SECURE_PARSING)
                             [deprecated: --secureParsing]

      --should-run=PARAM
                             Should Liquibase commands execute
                             DEFAULT: true
                             (liquibase.shouldRun)
                             (LIQUIBASE_SHOULD_RUN)
                             [deprecated: --shouldRun]

      --should-snapshot-data=PARAM

                             Should Liquibase snapshot data by default?
                             DEFAULT: false
                             (liquibase.shouldSnapshotData)
                             (LIQUIBASE_SHOULD_SNAPSHOT_DATA)
                             [deprecated: --shouldSnapshotData]

      --sql-log-level=PARAM
                             Level to log SQL statements to
                             DEFAULT: FINE
                             (liquibase.sql.logLevel)
                             (LIQUIBASE_SQL_LOG_LEVEL)
                             [deprecated: --sqlLogLevel]

      --sqlcmd-args=PARAM
                             Extra arguments which will be passed to the sqlcmd executable
                             (liquibase.sqlcmd.args)
                             (LIQUIBASE_SQLCMD_ARGS)
                             [deprecated: --sqlcmdArgs]

      --sqlcmd-catalog-name=PARAM

                             Database to use when running SQLCMD
                             (liquibase.sqlcmd.catalogName)
                             (LIQUIBASE_SQLCMD_CATALOG_NAME)
                             [deprecated: --sqlcmdCatalogName]

      --sqlcmd-executor=PARAM

                             Custom executor that can specified
                             (liquibase.sqlcmd.executor)
                             (LIQUIBASE_SQLCMD_EXECUTOR)
                             [deprecated: --sqlcmdExecutor]

      --sqlcmd-keep-temp=PARAM

                             If true, do not delete temporary sql files generated
                             DEFAULT: false
                             (liquibase.sqlcmd.keep.temp)
                             (LIQUIBASE_SQLCMD_KEEP_TEMP)
                             [deprecated: --sqlcmdKeepTemp]

      --sqlcmd-keep-temp-name=PARAM

                             Optionally, specify a custom filename
                             (liquibase.sqlcmd.keep.temp.name)
                             (LIQUIBASE_SQLCMD_KEEP_TEMP_NAME)
                             [deprecated: --sqlcmdKeepTempName]

      --sqlcmd-keep-temp-overwrite=PARAM

                             If true, overwrite any files in the specified directory with the same name
                             DEFAULT: true
                             (liquibase.sqlcmd.keep.temp.overwrite)
                             (LIQUIBASE_SQLCMD_KEEP_TEMP_OVERWRITE)
                             [deprecated: --sqlcmdKeepTempOverwrite]

      --sqlcmd-keep-temp-path=PARAM

                             Optionally, specify the path in which to store the temporary files. If not specified, the files will be
                               stored in the system's temp directory.
                             (liquibase.sqlcmd.keep.temp.path)
                             (LIQUIBASE_SQLCMD_KEEP_TEMP_PATH)
                             [deprecated: --sqlcmdKeepTempPath]

      --sqlcmd-log-file=PARAM

                             Log file for SQLCMD output
                             (liquibase.sqlcmd.logFile)
                             (LIQUIBASE_SQLCMD_LOG_FILE)
                             [deprecated: --sqlcmdLogFile]

      --sqlcmd-path=PARAM
                             Path to sqlcmd executable
                             (liquibase.sqlcmd.path)
                             (LIQUIBASE_SQLCMD_PATH)
                             [deprecated: --sqlcmdPath]

      --sqlcmd-timeout=PARAM
                             Seconds to wait for sqlcmd timeout
                             (liquibase.sqlcmd.timeout)
                             (LIQUIBASE_SQLCMD_TIMEOUT)
                             [deprecated: --sqlcmdTimeout]

